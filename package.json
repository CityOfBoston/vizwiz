{
  "name": "vizwiz",
  "version": "1.0.0",
  "description": "A data visualization widget library",
  "author": "Corey Oordt <coreyoordt@gmail.com>",
  "main": "dist/lib/vizwiz.min.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "npm-run-all --parallel dev:lib dev:server",
    "dev:lib": "webpack --config build/webpack.lib.conf.js --watch --progress --hide-modules",
    "dev:server": "node build/dev-server.js",
    "build": "npm run build:lib",
    "build:lib": "node build/build-lib.js",
    "build:app": "node build/build.js",
    "unit": "jest",
    "test": "npm run unit",
    "docs": "docsify serve docs",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^2.0.1",
    "chromedriver": "^2.27.2",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "deepmerge": "^2.0.1",
    "docsify": "^4.5.7",
    "docsify-cli": "^4.2.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "fecha": "^2.3.2",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "jest": "^22.0.4",
    "jest-vue-preprocessor": "^1.3.1",
    "karma": "^1.4.1",
