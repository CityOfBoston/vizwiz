{
  "name": "vizwiz",
  "version": "1.0.0",
  "description": "A data visualization widget library",
  "author": "Corey Oordt <coreyoordt@gmail.com>",
  "main": "dist/lib/vizwiz.min.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn run build",
    "dev": "npm-run-all --parallel dev:lib dev:server",
    "dev:lib": "webpack --config build/webpack.lib.conf.js --watch --progress --hide-modules",
    "dev:server": "node build/dev-server.js",
    "build": "yarn run build:lib",
    "build:lib": "node build/build-lib.js",
    "build:app": "node build/build.js",
    "unit": "jest",
    "test": "yarn run unit",
    "docs": "docsify serve docs",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {
    "nanoid": "^1.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "axios": "^0.17.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^22.4.2",
    "jest-vue-preprocessor": "^1.3.1",
    "npm-run-all": "^4.1.2",
    "opn": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.8.1",
    "templayed": "github:CityOfBoston/templayed.js",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "uikit": "^3.0.0-beta.40",
    "vue": "^2.3.3",
    "vue-custom-element": "^2.0.5",
    "vue-loader": "^14.1.1",
    "vue-select": "^2.3.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vue-template-es2015-compiler": "^1.6.0",
    "vue-test-utils": "^1.0.0-beta.9",
    "vuelidate": "^0.6.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "test/unit/coverage",
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "roots": [
      "<rootDir>/test/unit/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
